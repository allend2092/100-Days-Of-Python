    print("Welcome to Treasure Island!")
    print("Your mission is to find the treasure!")

    left_or_right = input("Do you want to go 'left' or 'right'? ")
    left_or_right = left_or_right.lower()
    print(type(left_or_right))
    print(left_or_right)

    if left_or_right.lower() == 'left':
        swim_or_weight = input("Do you want to 'swim' or 'wait'? ")
        if swim_or_weight.lower() == 'wait':
            which_door = input("Which door do you want to open? 'red' 'yellow' 'blue' ")
            if which_door.lower() == 'yellow':
                print('You win! Game Over!')
                exit(0)

            elif which_door.lower() == 'blue':
                print('You died! Game Over!')
                exit(0)
            else:
                print('You died! Game Over!')
                exit(0)


        else:
            print('You died! Game Over!')
            exit(0)
    else:
        print("You died! Game Over!")






